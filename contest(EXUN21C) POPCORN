#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{int a1,a2,b1,b2,c1,c2,A1,B1,C1;
	    cin>>a1>>a2;
	    cin>>b1>>b2;
	    cin>>c1>>c2;
	    A1=a1+a2;
	    B1=b1+b2;
	    C1=c1+c2;
	    if(A1>B1 && A1>C1)
	    cout<<A1<<endl;
	    else if(B1>A1 && B1>C1)
	    cout<<B1<<endl;
	    else
	    cout<<C1<<endl;
	}
	return 0;
}
