#include<bits/stdc++.h>
using namespace std;

int main() {
    int x;
    double y,n;
    cin>>x>>y;
    if((x>0&& x<=2000)&&(y>=0 && y<=2000)){
    if(x%5==0 && (x+0.50<=y))
    {
      n=y-(x+0.5);
     cout << fixed << setprecision(2) << n;
    }
    else
    cout << fixed << setprecision(2) << y;
    }
	return 0;
}
